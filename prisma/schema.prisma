generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int    @id @default(autoincrement())
  firstName String @map("FirstName") @db.VarChar(20)
  lastName  String @map("LastName") @db.VarChar(20)
  email     String @map("Email") @db.VarChar(40)
  password  String @map("Password") @db.VarChar(25)
  username  String @map("Username") @db.VarChar(30)
}
